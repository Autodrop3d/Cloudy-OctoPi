set -x
set -e

export LC_ALL=C

source /common.sh
install_cleanup_trap

echo_green "Installing system dependencies..."
apt install --yes  python3-pip

cd /home/pi
wget https://github.com/Autodrop3d/autodrop3dClient/raw/master/belt-engine.part
mv belt-engine.part belt-engine.tar.gz
pip3 install ./belt-engine.tar.gz

